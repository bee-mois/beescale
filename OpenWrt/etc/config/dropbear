config dropbear
	option PasswordAuth 'on'
	option Interface 'lan'
	option Port '2233'

config dropbear
	option Interface 'vpn'
	option Port '2233'
	option RootPasswordAuth 'off'
	option GatewayPorts 'on'
	option PasswordAuth 'off'

config dropbear
	option PasswordAuth 'on'
	option Interface 'wwan'
	option Port '2233'

config dropbear
	option PasswordAuth 'on'
	option Interface 'wan1'
	option Port '2233'
