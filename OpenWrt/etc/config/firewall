
config defaults
	option syn_flood '1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'

config zone
	option name 'lan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option network 'lan ethernet2lan'

config zone
	option name 'wan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'REJECT'
	option network 'wan wan6 congstar'

config zone
	option name 'wan1'
	option network 'wan1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'REJECT'

config zone
	option name 'wan2'
	option output 'ACCEPT'
	option masq '1'
	option mtu_fix '1'
	option input 'ACCEPT'
	option forward 'ACCEPT'
	option network 'IN_VPN'

config redirect
	option target 'DNAT'
	option dest 'lan'
	option src_dport '2233'
	option dest_port '2233'
	option name 'ssh'
	option src 'wan2'
	option proto 'tcp'

config rule
	option target 'ACCEPT'
	option dest_port '2233'
	option name 'ssh'
	option family 'ipv4'
	option proto 'tcp'
	option src 'wan2'

config forwarding
	option dest 'wan2'
	option src 'lan'

config forwarding
	option dest 'lan'
	option src 'wan2'

config rule
	option target 'ACCEPT'
	option src 'wan2'
	option dest 'lan'
	option name 'ssh'
	option family 'ipv4'
	option proto 'tcp'
	option src_port '2233'
	option dest_port '2233'

config include
	option path '/etc/firewall.freifunk'

config redirect
	option target 'DNAT'
	option dest 'lan'
	option proto 'tcp udp'
	option name 'ssh6'
	option src 'lan'

config rule
	option target 'ACCEPT'
	option proto 'tcp'
	option name 'ssh6'
	option family 'ipv6'
	option src 'lan'

config rule
	option target 'ACCEPT'
	option src 'lan'
	option dest 'lan'
	option name 'ssh6'
	option family 'ipv6'
	option proto 'tcp'
